Wrapper class Boxing:

import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    int a=10;
	    Integer obj1=a;
	   
	    char ch='d';
	    Character obj2=ch;
	    
	    float f=10.5f;
	    Float obj3=f;
	    
	    
	    boolean b1 = Boolean.parseBoolean("True");
        Boolean obj4=b1;
        
        short s=100;
        Short obj5=s;
        
        byte b= 6;
        Byte obj6= b;
        
        System.out.print(obj6);
	}
}

unboxing wrapper class:
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	   
	    Integer obj1=new Integer(10);
	    int a=obj1;
	    
	    Character obj2=new Character('b') ;
	    char ch=Character.valueOf(obj2);
	   
	    Float obj3=new Float(10.5f);
	    float f=obj3;
	    
        Boolean obj4=Boolean.parseBoolean("True");
         boolean b1=obj4;
        
         Short s = new Short(100);
        short value = s.shortValue();
        //Byte obj6= Byte(6);
        //byte b=obj6;
        
        System.out.println(a);
        System.out.println(ch);
        System.out.println(f); 
        System.out.println(b1);
        System.out.println(s);
        System.out.print(b);
	}
}


